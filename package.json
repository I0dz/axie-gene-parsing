{
  "name": "agp-ts",
  "version": "0.0.1-test",
  "author": "Shane Maglangit",
  "license": "GPL-3.0",
  "description": "Gene parsing package for Axie Infinity.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "coverage": "nyc npm test",
    "docs": "typedoc src --out docs",
    "develop": "tsc && node dist/src/axie-gene.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jest.config.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "axie",
    "axie-infinity",
    "gene"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "jest": "^27.0.6",
    "nyc": "^15.1.0",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "@types/node": "^16.4.3",
    "prettier": "^2.3.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5",
    "typedoc": "^0.21.4"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShaneMaglangit/agp-ts.git"
  },
  "bugs": {
    "url": "https://github.com/ShaneMaglangit/agp-ts/issues"
  },
  "homepage": "https://github.com/ShaneMaglangit/agp-ts#readme"
}
